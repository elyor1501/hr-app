# D:\hr-app\services\backend\.env.example

# Application
HR_APP_APP_NAME=hr-app-backend
HR_APP_ENVIRONMENT=development
HR_APP_HOST=0.0.0.0
HR_APP_PORT=8000
HR_APP_LOG_LEVEL=INFO

# CORS
HR_APP_CORS_ORIGINS=["http://localhost:3000"]

# Database
HR_APP_DATABASE_HOST=localhost
HR_APP_DATABASE_PORT=5432
HR_APP_DATABASE_USER=postgres
HR_APP_DATABASE_PASSWORD=postgres
HR_APP_DATABASE_NAME=hr_app_backend

# Connection Pool
HR_APP_DATABASE_POOL_SIZE=5
HR_APP_DATABASE_MAX_OVERFLOW=10
HR_APP_DATABASE_POOL_TIMEOUT=30

# Vector Settings (768 for Gemini, 1536 for OpenAI)
HR_APP_VECTOR_DIMENSION=768